@import super();

.info-title {
  alignment-x: center;
  text-color: green;
  font-size: 1.5;
  top: 0;
}

.info-container {
  top: 20;
  bottom: 20;
  left: 20;
  right: 20;
  filter: blur(25);
  border-radius: 15;
  border-thickness: 5;
  border-color: var(--background-color-border);
  background-color: var(--background-color);
  box-shadow: none;
}

.commands-title {
  text-color: green;
  font-size: 1.5;
  top: 10;
  left: 10;
}

.commands {
  top: 30;
  left: 20;
}