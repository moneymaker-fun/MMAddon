.title {
  alignment-x: center;
  text-color: green;
  font-size: 1.5;
  top: 0;
}

.container {
  top: 20;
  bottom: 20;
  left: 20;
  right: 20;
  filter: blur(25);
  border-radius: 15;
  border-thickness: 5;
  border-color: var(--background-color-border);
  background-color: var(--background-color);
  box-shadow: none;
}